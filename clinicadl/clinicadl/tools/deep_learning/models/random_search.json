{
    "mode": "slice",
    "network_type": "cnn",
    "caps_dir": "/path/to/caps",
    "preprocessing": "t1-linear",
    "tsv_path": "/path/to/tsv",
    "diagnoses": ["AD", "CN"],
    "baseline": false,
    "unnormalize": false,
    "data_augmentation": false,
    "sampler": "random",
    "epochs": 100,
    "learning_rate": [2, 5],
    "wd_bool": [true, false],
    "weight_decay": [2, 4],
    "dropout": [0, 0.9],
    "patience": 20,
    "tolerance": 0,
    "accumulation_steps": 1,
    "evaluation_steps": 0,
    "n_convblocks": [2, 6],
    "first_conv_width": [2, 5],
    "channels_limit": 256,
    "d_reduction": "MaxPooling",
    "n_conv": [1, 2],
    "n_fcblocks": [1, 3],
}
